				"noGlobalEval": "error"
			},
			"style": {
				"noCommonJs": "off",
				"noDefaultExport": "off",
				"noNamespace": "error",
				"noNegationElse": "off",
				"noNestedTernary": "error",
				"noParameterAssign": "error",
